# 이름 : 김철학
# 내용 : Ch01. R설치와 개요 - 변수 교재 p 39

# 숫자형
num1 <- 1
num2 <- 2
num3 <- 3.14
num1
num2;num3

# 논리형
bool1 <- TRUE
bool2 <- FALSE
bool1;bool2

# 문자형
str1 <- "Hello"
str2 <- 'Welcome'
str1
str2

# 특수형
data1 <- NULL # 초기값을 의미
data2 <- NA # 결측값, 누락된값 Not Available
data3 <- NaN # 숫자가 아닌 값, Not a Number

data1
data2
data3

